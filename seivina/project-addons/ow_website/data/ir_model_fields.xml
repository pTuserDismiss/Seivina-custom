<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <function model="ir.model.fields" name="formbuilder_whitelist">
        <value>sale.order</value>
        <value eval="[
            'distributor_name',
            'other_distributor',
            'shipping_service_level',
            'business_type',
            'tax_exempt',
        ]"/>
    </function>
    <function model="ir.ui.view" name="write">
        <value model="ir.ui.view" search="[('key', '=', 'website_sale_comparison.product_attributes_body')]"/>
        <value eval="{'active': False}"/>
    </function>
</odoo>
