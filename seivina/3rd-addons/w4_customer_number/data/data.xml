<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_res_partner" model="ir.sequence">
                <field name="name">Partner/Customer</field>
                <field name="code">res.partner</field>
                <field name="prefix">K</field>
                <field name="padding">5</field>
                <field name="number_next_actual">10000</field>
                <field name="company_id" eval="False"/>
        </record>

        <record id="cron_update_customer_number_ref" model="ir.cron">
            <field name="name">Create customer numbers</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model.cron_update_customer_number_ref()</field>
        </record>
    </data>
</odoo>
